    "    user=\"root\",\n",
    "    passwd='my-secret-pw',\n",
    "    db=\"mysql\",\n",
    "    charset='utf8mb4')\n",
    "\n",
    "df = pd.read_sql_query(\"SELECT * FROM user\",\n",
    "    conn)\n",
    "df.tail(10)"
